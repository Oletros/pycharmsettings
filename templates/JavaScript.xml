<templateSet group="JavaScript">
  <template name="log;" value="console.log($EXP$);" description="Console log expression" toReformat="true" toShortenFQNames="true">
    <variable name="EXP" expression="" defaultValue="''" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="funcn" value="function $NAME$ ($PARAM$) {&#10;  $END$&#10;}" description="Insert function expression with name" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="''" alwaysStopAt="true" />
    <variable name="PARAM" expression="" defaultValue="''" alwaysStopAt="true" />
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>